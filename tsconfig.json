{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/layout/header/Header.jsx",
    "rules/login.validation.js",
    "services/users/index.js",
    "services/products/index.js",
    "components/layout/header/Header2.jsx",
    "components/form/index.jsx",
    "components/buttons/BtnProfile.jsx",
    "components/buttons/BtnAddCart.jsx",
    "components/buttons/Btn.jsx",
    "helpers/Axios.jsx",
    "hooks/useFormValidation.jsx",
    "pages/Profile.jsx",
    "pages/MyOrders.jsx",
    "pages/Cart.jsx",
    "pages/Login.jsx",
    "pages/index.jsx"
  ],
  "exclude": ["node_modules"]
}
